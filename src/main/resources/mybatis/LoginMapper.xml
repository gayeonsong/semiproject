<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.LoginMapper">
    <select id="login" parameterType="map" resultType="LoginDto">
        select user_id, login_id, role from login
        where login_id=#{login_id} and password=#{password}
    </select>
</mapper>